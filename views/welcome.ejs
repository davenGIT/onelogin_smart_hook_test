<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body text-center">
      <h1><i class="fas fa-bullseye fa-3x"></i></h1>
      <div id="divHidden" style="display:none">
        <input type="hidden" id="client_id" value="<%- hidclient_id %>" />
        <input type="hidden" id="subdomain" value="<%- hidsubdomain %>" />
      </div>
      <p>Test OneLogin Smart Hooks</p>
      <p></p>
      <div id="divLogin" style="display:<%- divLogin %>">
        <button type="submit" id="btnLogin" class="btn btn-primary btn-block">Login</button>
      </div>
      <div id="divLogout" style="display:<%- divLogout %>">
        <a href="" id="logoutlink">Click to Log Out</a>
      </div>
      <div>
        <p></p>
        <p style="text-align:left;"><b>Data</b></p>
        <p></p>
        <div style="text-align:left;" name="loginData" id="loginData"></div>
      </div>
    </div>
  </div>
</div>
